{
    "name": "biosyntax",
    "displayName": "bioSyntax",
    "description": "bioSyntax for computational biology",
    "repository": "https://github.com/liyao001/bioSyntax",
    "version": "0.0.11",
    "publisher": "reageyao",
    "license": "GPL-3.0",
    "icon": "https://github.com/liyao001/bioSyntax/raw/vscode/vscode/resources/logo.png",
    "engines": {
        "vscode": "^1.45.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "BioSyntax",
            "properties": {
                "BioSyntax.nonBioTheme": {
                    "type": "string",
                    "scope": "resource",
                    "default": "Default Dark+",
                    "description": "The theme to use when not viewing a BioSyntax document. Most people will want to set this to be the same as their 'workbench.colorTheme' setting"
                },
                "BioSyntax.autoSwitchTheme": {
                    "type": "boolean",
                    "scope": "resource",
                    "default": true,
                    "description": "Enable automatically switching to BioSyntax's color theme when opening a BioSyntax-compatible file"
                }
            }
        },
        "languages": [
            {
                "id": "bed",
                "aliases": [
                    "bed"
                ],
                "extensions": [
                    ".bed"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "clustal",
                "aliases": [
                    "clustal",
                    "aln"
                ],
                "extensions": [
                    ".clustal",
                    ".aln"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "cwl",
                "aliases": [
                    "cwl"
                ],
                "extensions": [
                    ".cwl"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "faidx",
                "aliases": [
                    "faidx"
                ],
                "extensions": [
                    ".fai"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "fasta",
                "aliases": [
                    "fasta",
                    "fasta"
                ],
                "extensions": [
                    ".fasta",
                    ".fa"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "fasta-clustal",
                "aliases": [
                    "fasta-clustal",
                    "fasta"
                ],
                "extensions": [
                    ".fastaa",
                    ".faa"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "fasta-hc",
                "aliases": [
                    "fasta-hc",
                    "fasta"
