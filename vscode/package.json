            },
            {
                "id": "fasta-taylor",
                "aliases": [
                    "fasta-taylor"
                ],
                "extensions": [
                    ".fastaylor"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "fasta-zappo",
                "aliases": [
